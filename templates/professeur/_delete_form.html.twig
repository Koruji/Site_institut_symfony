<form method="post" action="{{ path('app_professeur_delete', {'id': professeur.id}) }}" onsubmit="return confirm('Etes-vous sûr de vouloir supprimer votre compte ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ professeur.id) }}">
    <button class="btn btn-danger">Supprimer définitivement mon compte</button>
</form>
